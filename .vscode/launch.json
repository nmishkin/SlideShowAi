{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "args": [
                "pi",
                "mishkin",
                "Pictures/photo-frame",
                "/Users/mishkin/Pictures/Misc/For Slide Show/Photo Frame Base",
                "/Users/mishkin/Pictures/Misc/For Slide Show/Misc. Downloads",
                "/Users/mishkin/Pictures/Misc/For Slide Show/Photo Frame"
            ]
        }
    ]
}